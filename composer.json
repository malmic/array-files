{
    "name": "halimtuhu/array-files",
    "description": "A Laravel Nova field for distribute your files as array of object.",
    "keywords": [
        "laravel",
        "nova",
        "files",
        "array"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Halimtuhu\\ArrayFiles\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Halimtuhu\\ArrayFiles\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
